<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.convalidapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />

    <application
        android:name=".commons.MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_foreground"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_foreground"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.asignatura_prof.modulo_detail.UsuariosConvalidadosModuloDetailActivity"></activity>
        <activity android:name=".ui.users.DetalleUsersActivity" />
        <activity android:name=".ui.qr.GenerateQrActivity"></activity>
        <activity android:name=".ui.qr.QRActivity" />
        <activity
            android:name=".MainProfesorActivity"
            android:label="@string/title_activity_main_profesor" />
        <activity
            android:name=".MainConserjeActivity"
            android:label="@string/title_activity_main_conserje" />
        <activity android:name=".ui.asignatura.AsignaturaConvalidadaDetalleActivity" />
        <activity android:name=".ui.auth.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" />
    </application>

</manifest>